{
    "cluster_name": "jrx-onprem",
    "exhibitor_storage_backend": "zookeeper",
    "exhibitor_zk_hosts": "{{ workstation_ip }}:20181",
    "exhibitor_zk_path": "/jrx-onprem",
    "bootstrap_url":"file:///tmp/dcos",
    "resolvers": "{{ resolvers }}",
    "master_discovery":"static",
    "master_list": "[{% set pipe = joiner(',') %}{% for host in groups['masters'] %}{{ pipe() }}{{ '\\\"' + host + '\\\"' }}{% endfor %}]",
    "roles": "slave_public",
    "weights": "slave_public=1"
}
